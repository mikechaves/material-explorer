{
  "name": "material-explorer",
  "version": "1.0.0-beta",
  "homepage": "https://mikechaves.github.io/material-explorer",
  "private": true,
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@radix-ui/react-dialog": "^1.0.5",
    "@react-three/drei": "^9.102.6",
    "@react-three/fiber": "^8.16.0",
    "autoprefixer": "^10.4.18",
    "framer-motion": "^10.16.4",
    "lz-string": "^1.5.0",
    "postcss": "^8.4.37",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^9.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-resizable": "^3.0.5",
    "tailwindcss": "^3.4.1",
    "three": "^0.162.0",
    "typescript": "^4.9.5",
    "uuid": "^9.0.1",
    "web-vitals": "^2.1.4"
  },
  "overrides": {
    "node-forge": "^1.3.2",
    "glob": "^10.5.0",
    "qs": "^6.14.1",
    "jsonpath": "^1.2.1",
    "brace-expansion": "^1.1.12"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"tests/**/*.{ts,tsx}\" \"vite.config.ts\" \"playwright.config.ts\" --max-warnings=0",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "quality:ci": "npm run check-format && npm run lint && npm run type-check",
    "security:audit": "npm audit --audit-level=high",
    "quality:full": "npm run quality:ci && npm run test:ci && npm run build && npm run check:bundle && npm run test:e2e && npm run security:audit",
    "check:bundle": "node scripts/check-bundle-budget.js",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:a11y": "playwright test tests/e2e/accessibility.spec.ts",
    "postinstall": "node scripts/fix-mediapipe-sourcemaps.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.1",
    "@playwright/test": "^1.58.2",
    "@types/node": "^20.19.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-resizable": "^3.0.7",
    "@types/three": "^0.162.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.8.1",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
